	{
		"name": "Safebit",
		"version": "0.31",
		"background_page": "background.html",
		"permissions": [
			"tabs", "http://*/*", "https://*/*",
			"clipboardWrite", "notifications"
		],
		"icons": {
			"128": "static/icon/128.png",
			"16": "static/icon/16.png"
		},
		"minimum_chrome_version": "4.1",
		"app": {
			"launch": {
				"local_path": "view/main.html",
				"height": 720
			}	
		}
	}